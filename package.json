{
  "name": "svelte5-typescript-bun-template",
  "version": "0.0.0",
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.34",
    "@iconify-json/ph": "^1.1.13",
    "@julr/unocss-preset-forms": "^0.1.0",
    "@julr/unocss-preset-heropatterns": "^1.0.2",
    "@playwright/test": "^1.44.1",
    "@sveltejs/adapter-auto": "^3.2.1",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.5.10",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@types/bun": "latest",
    "@unocss/svelte-scoped": "^0.59.4",
    "buffer": "^6.0.3",
    "commit-and-tag-version": "^12.4.1",
    "isomorphic-dompurify": "^2.11.0",
    "lz-string": "^1.5.0",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "smplr": "^0.13.4",
    "svelte": "^5.0.0-next.144",
    "svelte-check": "^3.8.0",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "unocss": "^0.59.4",
    "unocss-preset-theme": "^0.12.0",
    "vite": "^5.2.12"
  },
  "private": true,
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "bun test --watch",
    "e2e": "playwright test",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "deploy": "bunx gh-pages -d build --dotfiles",
    "release": "commit-and-tag-version",
    "postrelease": "git push --follow-tags origin main"
  },
  "type": "module"
}
